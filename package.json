{
  "name": "fillominordle",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "fix": "prettier --write src",
    "lint": "prettier --check src",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "prepare": "husky install",
    "ort:analyse": "docker run -v \"$(pwd)\":/project philipssoftware/ort --info analyze -f JSON -i /project -o /project/ort",
    "ort:report": "docker run -v \"$(pwd)\":/project philipssoftware/ort --info report -f NoticeTemplate -i /project/ort/analyzer-result.json -o /project/ort"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "classnames": "^2.3.1",
    "grapheme-splitter": "1.0.4",
    "lodash": "^4.17.21",
    "random-seed": "^0.3.0",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.4",
    "web-vitals": "^2.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.179",
    "@types/node": "^16.11.19",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "tailwindcss": "^3.0.12",
    "ts-node": "^10.5.0"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js,jsx,css,md}": "prettier --write"
  }
}
